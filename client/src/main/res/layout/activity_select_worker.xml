<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tl="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/header" />

    <!--<include layout="@layout/select_worker" />-->

    <!--<com.amap.api.maps.MapView-->
    <!--android:id="@+id/aMapview"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent" />-->
    <com.flyco.tablayout.SlidingTabLayout
        android:id="@+id/slidingtablayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_100"
        android:background="#fff"
        app:tl_divider_color="@color/color_service_deiveder"
        app:tl_divider_width="@dimen/dimen_1"
        tl:tl_indicator_color="@color/color_service_title"
        tl:tl_tab_padding="0dp"
        tl:tl_tab_space_equal="true"
        tl:tl_textSelectColor="@color/color_service_title"
        tl:tl_textUnselectColor="@color/color_client_neworder"
        tl:tl_textsize="@dimen/textSize_13" />

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_selectWork"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>