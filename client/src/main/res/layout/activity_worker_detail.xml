<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/header" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_white"
                android:orientation="horizontal"
                android:padding="5dp"
                android:visibility="visible">

                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/iv_header"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center_vertical"
                    fresco:placeholderImage="@mipmap/ic_nodata"
                    fresco:roundAsCircle="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_realname"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="李白"
                                android:textColor="@color/color_client_neworder"
                                android:textSize="@dimen/textSize_15" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_20"
                                android:text="技师"
                                android:textColor="@color/color_service_title"
                                android:textSize="@dimen/textSize_12" />
                        </LinearLayout>

                        <me.zhanghai.android.materialratingbar.MaterialRatingBar
                            android:id="@+id/rb_evalute"
                            style="@style/Widget.MaterialRatingBar.RatingBar"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/dimen_46"
                            android:layout_alignParentRight="true"
                            android:isIndicator="true"
                            android:maxHeight="20dp"
                            android:minHeight="20dp"
                            android:progressBackgroundTint="@color/font_color"
                            android:progressTint="#FFA53A"
                            android:rating="5"
                            android:secondaryProgressTint="@color/font_color"
                            android:stepSize="0.1" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/tv_company_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="北京某某某科技有限公司"
                        android:textColor="@color/color_client_neworder"
                        android:textSize="@dimen/textSize_13" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="编号:"
                            android:textColor="@color/color_client_neworder"
                            android:textSize="@dimen/textSize_13" />

                        <TextView
                            android:id="@+id/tv_code"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen_10"
                            android:layout_marginTop="10dp"
                            android:text="111"
                            android:textColor="@color/color_client_neworder"
                            android:textSize="@dimen/textSize_13" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <View style="@style/style_common_line_gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_white"
                android:paddingBottom="@dimen/dimen_36"
                android:paddingTop="@dimen/dimen_34"
                android:visibility="visible">

                <com.google.android.flexbox.FlexboxLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/dimen_64"
                    custom:flexDirection="row"
                    custom:flexWrap="wrap"
                    custom:justifyContent="flex_start">

                    <TextView
                        style="@style/style_work_detail_type"
                        android:text="哈哈" />

                    <TextView
                        style="@style/style_work_detail_type"
                        android:text="的法哈" />

                    <TextView
                        style="@style/style_work_detail_type"
                        android:text="哈我让去哈" />

                    <TextView
                        style="@style/style_work_detail_type"
                        android:text="哈我让去哈" />

                    <TextView
                        style="@style/style_work_detail_type"
                        android:text="哈我让去哈" />
                </com.google.android.flexbox.FlexboxLayout>
            </RelativeLayout>

            <View style="@style/style_common_line_gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_white">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dimen_30"
                        android:text="@string/text_worker_detail_see_more_type"
                        android:textColor="@color/color_bottom"
                        android:textSize="@dimen/textSize_12" />

                    <ImageView
                        android:id="@+id/iv_workerDetailDown"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingLeft="@dimen/dimen_10"
                        android:paddingRight="@dimen/dimen_10"
                        android:src="@mipmap/ic_worker_detail_type_down" />

                    <ImageView
                        android:id="@+id/iv_workerDetailUp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingLeft="@dimen/dimen_10"
                        android:paddingRight="@dimen/dimen_10"
                        android:src="@mipmap/ic_worker_detail_type_up" />
                </LinearLayout>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="@color/color_white"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_26"
                        android:layout_marginTop="@dimen/dimen_26"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/ic_worker_detail_repair_num" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/dimen_30"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_number"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0单"
                                android:textColor="@color/color_service_title"
                                android:textSize="@dimen/textSize_13" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimen_14"
                                android:text="维修数量"
                                android:textColor="@color/color_client_neworder"
                                android:textSize="@dimen/textSize_13" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/style_common_line_vertical_gray" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_26"
                        android:layout_marginTop="@dimen/dimen_26"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/ic_worker_detail_grade" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/dimen_30"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_koubei"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="5分"
                                android:textColor="@color/color_service_title"
                                android:textSize="@dimen/textSize_13" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dimen_14"
                                    android:text="口碑"
                                    android:textColor="@color/color_client_neworder"
                                    android:textSize="@dimen/textSize_13" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="口碑"
                                    android:textColor="@color/color_client_neworder"
                                    android:textSize="@dimen/textSize_13"
                                    android:visibility="invisible" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View style="@style/style_common_line_gray" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/color_white"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_26"
                        android:layout_marginTop="@dimen/dimen_26"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/ic_worker_detail_ability" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/dimen_30"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_level"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="一级"
                                android:textColor="@color/color_service_title"
                                android:textSize="@dimen/textSize_13" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimen_14"
                                android:text="能力等级"
                                android:textColor="@color/color_client_neworder"
                                android:textSize="@dimen/textSize_13" />
                        </LinearLayout>
                    </LinearLayout>

                    <View style="@style/style_common_line_vertical_gray" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_26"
                        android:layout_marginTop="@dimen/dimen_26"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/ic_worker_detail_work_time" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/dimen_30"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_year"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="8年"
                                android:textColor="@color/color_service_title"
                                android:textSize="@dimen/textSize_13" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimen_14"
                                android:text="从业年限"
                                android:textColor="@color/color_client_neworder"
                                android:textSize="@dimen/textSize_13" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_14"
                android:background="@drawable/bg_ll_selctor"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/dimen_20"
                android:paddingLeft="@dimen/dimen_38"
                android:paddingTop="@dimen/dimen_20">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="工作地址:"
                    android:textColor="@color/color_client_neworder"
                    android:textSize="@dimen/textSize_14" />

                <TextView
                    android:id="@+id/tv_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text=""
                    android:textColor="@color/color_client_neworder"
                    android:textSize="@dimen/textSize_14" />
            </LinearLayout>

            <View style="@style/style_common_line_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_ll_selctor"
                android:orientation="vertical"
                android:paddingBottom="@dimen/dimen_20"
                android:paddingLeft="@dimen/dimen_38"
                android:paddingTop="@dimen/dimen_20">

                <LinearLayout
                    android:id="@+id/ll_area"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_ll_selctor"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="服务区域"
                        android:textColor="@color/color_client_neworder"
                        android:textSize="@dimen/textSize_14" />

                    <ImageView
                        android:id="@+id/iv_down"
                        android:layout_width="18dp"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/dimen_46"
                        android:src="@mipmap/ic_worker_detail_area_down" />
                </LinearLayout>


            </LinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_list1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:visibility="gone" />

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_list2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone" />

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_list3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_14"
                android:background="@drawable/bg_ll_selctor"
                android:paddingBottom="@dimen/dimen_20"
                android:paddingLeft="@dimen/dimen_38"
                android:paddingTop="@dimen/dimen_20"
                android:text="荣誉证书"
                android:textColor="@color/color_client_neworder"
                android:textSize="@dimen/textSize_14" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dimen_38">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/iv_pic1"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        fresco:placeholderImage="@mipmap/ic_nodata" />

                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/iv_pic2"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        fresco:placeholderImage="@mipmap/ic_nodata" />

                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/iv_pic3"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        fresco:placeholderImage="@mipmap/ic_nodata" />

                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/iv_pic4"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        fresco:placeholderImage="@mipmap/ic_nodata" />
                </LinearLayout>
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_white"
                android:paddingBottom="@dimen/dimen_20"
                android:paddingLeft="@dimen/dimen_38"
                android:paddingTop="@dimen/dimen_20">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_ll_selctor"
                    android:text="客户评价"
                    android:textColor="@color/color_client_neworder"
                    android:textSize="@dimen/textSize_14" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_margin="5dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="好评率："
                        android:textColor="@color/roll_content"
                        android:textSize="@dimen/textSize_12" />

                    <com.daimajia.numberprogressbar.NumberProgressBar
                        android:id="@+id/iv_haopinglv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:visibility="gone"
                        custom:progress_current="100"
                        custom:progress_max="100"
                        custom:progress_reached_color="@color/client"
                        custom:progress_text_visibility="invisible"
                        custom:progress_unreached_color="#CCCCCC" />

                    <TextView
                        android:id="@+id/tv_haopinglv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/dimen_10"
                        android:layout_marginRight="@dimen/dimen_46"
                        android:text="100%"
                        android:textColor="@color/roll_content"
                        android:textSize="@dimen/textSize_12" />

                </LinearLayout>

            </RelativeLayout>

            <View style="@style/style_common_line_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_white"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_32"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_20"
                    android:paddingLeft="@dimen/dimen_38"
                    android:paddingTop="@dimen/dimen_20">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="解释清故障原因及处理措施"
                        android:textColor="@color/roll_content"
                        android:textSize="@dimen/textSize_13" />

                    <me.zhanghai.android.materialratingbar.MaterialRatingBar
                        android:id="@+id/rb_star1"
                        style="@style/Widget.MaterialRatingBar.RatingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_46"
                        android:isIndicator="true"
                        android:maxHeight="20dp"
                        android:minHeight="20dp"
                        android:progressBackgroundTint="@color/font_color"
                        android:progressTint="#FFA53A"
                        android:rating="5"
                        android:secondaryProgressTint="@color/font_color"
                        android:stepSize="0.1" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_ll_selctor"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_20"
                    android:paddingLeft="@dimen/dimen_38"
                    android:paddingTop="@dimen/dimen_20">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="服务态度热情友好"
                        android:textColor="@color/roll_content"
                        android:textSize="@dimen/textSize_13" />

                    <me.zhanghai.android.materialratingbar.MaterialRatingBar
                        android:id="@+id/rb_star2"
                        style="@style/Widget.MaterialRatingBar.RatingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_46"
                        android:isIndicator="true"
                        android:maxHeight="20dp"
                        android:minHeight="20dp"
                        android:progressBackgroundTint="@color/font_color"
                        android:progressTint="#FFA53A"
                        android:rating="5"
                        android:secondaryProgressTint="@color/font_color"
                        android:stepSize="0.1" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_ll_selctor"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_20"
                    android:paddingLeft="@dimen/dimen_38"
                    android:paddingTop="@dimen/dimen_20">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="作业符合规范"
                        android:textColor="@color/roll_content"
                        android:textSize="@dimen/textSize_13" />

                    <me.zhanghai.android.materialratingbar.MaterialRatingBar
                        android:id="@+id/rb_star3"
                        style="@style/Widget.MaterialRatingBar.RatingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_46"
                        android:isIndicator="true"
                        android:maxHeight="20dp"
                        android:minHeight="20dp"
                        android:progressBackgroundTint="@color/font_color"
                        android:progressTint="#FFA53A"
                        android:rating="5"
                        android:secondaryProgressTint="@color/font_color"
                        android:stepSize="0.1" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_ll_selctor"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_20"
                    android:paddingLeft="@dimen/dimen_38"
                    android:paddingTop="@dimen/dimen_20">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="技术能力水平"
                        android:textColor="@color/roll_content"
                        android:textSize="@dimen/textSize_13" />

                    <me.zhanghai.android.materialratingbar.MaterialRatingBar
                        android:id="@+id/rb_star4"
                        style="@style/Widget.MaterialRatingBar.RatingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_46"
                        android:isIndicator="true"
                        android:maxHeight="20dp"
                        android:minHeight="20dp"
