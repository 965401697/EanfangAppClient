    <style name="style_client_home_meun">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color_client_menu</item>
        <item name="android:textSize">@dimen/textSize_12</item>
    </style>

    <style name="style_client_home_meun_new">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_client_menu</item>
        <item name="android:textSize">@dimen/textSize_12</item>
    </style>

    <style name="style_common_line_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_1</item>
        <item name="android:background">@color/color_new_line</item>
    </style>

    <style name="style_common_line_vertical_gray">
        <item name="android:layout_width">@dimen/dimen_1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/color_data_line</item>
    </style>

    <style name="style_order_line_vertical_gray">
        <item name="android:layout_width">@dimen/dimen_1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/color_order_status_line</item>
    </style>

    <style name="style_client_work_meun">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">@dimen/dimen_20</item>
        <item name="android:textColor">@color/color_client_menu</item>
        <item name="android:textSize">@dimen/textSize_12</item>
    </style>
    <!--  自定义dialog style -->
    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <declare-styleable name="SideBar">
        <attr name="scaleSize" format="integer" />
        <attr name="scaleItemCount" format="integer" />
        <attr name="scaleWidth" format="dimension" />
    </declare-styleable>

    <!-- 右上角popupwindow -->
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <!--  -->
    <style name="Theme.picker" parent="@android:style/Theme.Holo.Light">
        <item name="android:textColor">#333333</item>
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_show</item>
        <item name="android:windowExitAnimation">@anim/pop_hide</item>
    </style>

    <!-- 技师详情  -->
    <style name="style_worker_detail_order">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingTop">@dimen/dimen_10</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="style_worker_detail_order_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">@dimen/dimen_28</item>
        <item name="android:textColor">@color/color_client_neworder</item>
        <item name="android:textSize">@dimen/textSize_13</item>
    </style>
</resources>
